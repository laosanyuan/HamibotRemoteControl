<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="BackgroundColor.Primary">#242C3B</Color>
    <Color x:Key="BackgroundColor.Secondary">#4B4CED</Color>
    <Color x:Key="BackgroundColor.Tertiary">#37B6E9</Color>
    <Color x:Key="BackgroundColor.Forbidden">#919191</Color>

    <Color x:Key="Foreground.Primary">White</Color>
    <Color x:Key="Foreground.Secondary">#919191</Color>

    <!--成功-->
    <Color x:Key="SuccessColor">#4FE6AF</Color>
    <!--失败-->
    <Color x:Key="FailColor">#ED1C24</Color>


    <!--tag颜色-->
    <Color x:Key="TagBackground">#37B6E9</Color>
    <Color x:Key="TagForeground">#242C3B</Color>

</ResourceDictionary>